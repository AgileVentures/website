<% content_for :title do %>The team<% end %>
<section id="contributors">
<h2>The team</h2>
<p>Agile Ventures team:</p>
<ul class="media-list">
  <% @contributors[0...10].each  do |contributor| %>
      <li class="media">
        <a class="pull-left" href="<%= contributor['html_url'] %>">
          <img class="media-object img-rounded" src="<%= contributor['avatar_url'] %>" style="width:75px;height:75px">
        </a>
        <div class="media-body">
          <h4 class="media-heading"><%= contributor['login'] %></h4>
          <a href="<%= contributor['html_url'] %>"> <%= contributor['html_url'] %></a>
          <!--p class="media-heading"> <%= contributor['contributions'] %> contributions.</p-->
        </div>
      </li>
  <% end %>
</ul>
</section>